{
  "name": "fml",
  "version": "1.0.0",
  "description": "A food market location app",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev:server": "nodemon --watch src --exec babel-node -- src/server/app.js ",
    "build:server": "babel ./src/server --out-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phemmz/fml.git"
  },
  "keywords": [
    "food",
    "market",
    "location"
  ],
  "author": "Femi Adetunji",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phemmz/fml/issues"
  },
  "homepage": "https://github.com/phemmz/fml#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.3",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.7"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0"
  }
}
